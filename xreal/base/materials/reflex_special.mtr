
textures/reflex/special/wallwalk01_blue
{
    qer_editorimage  textures/reflex/special/wallwalk01.tga
    
    xmap_surfaceLight 800
	xmap_lightRGB 0.24 0.41 0.77
	xmap_lightImage textures/reflex/special/wallwalk01_glow.tga
      xmap_lightSubdivide 16

    
    wallwalk
    
    diffusemap       textures/reflex/special/wallwalk01.tga
    specularMap      textures/reflex/special/wallwalk01_spec.tga
    bumpmap          textures/reflex/special/wallwalk01_local.tga
	{
		blend		add
		map	      textures/reflex/special/wallwalk01_glow
        color       0.24 * (jitter5 [time * 2]), 0.41 * (jitter5 [time * 2]), 0.77 * (jitter5 [time * 2]), 1.0
	}
}

