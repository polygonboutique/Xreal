textures/reflex/special/glass01
{

qer_editorimage  cubemaps/reflex/glass01_nx

	noshadows
	twosided
      translucent

	{
                        stage reflectionmap
            blend       gl_dst_alpha, gl_one
		cubeMap	cubemaps/reflex/glass01
	}
}


textures/reflex/special/launchpad01
{
    qer_editorimage  textures/reflex/special/launchpad01 

    xmap_surfaceLight 800
	xmap_lightRGB 0.94 0.57 0.13
	xmap_lightImage textures/reflex/special/wallwalk01_glow.tga
      xmap_lightSubdivide 16

      {
            blend diffusemap
            map textures/reflex/special/launchpad01_treads
            scroll    time * -1.4, time* 0.0
      }
	{
		blend specularMap      
		map textures/reflex/special/launchpad01_treads_spec
            scroll    time * -1.4, time* 0.0
	}
	{
		blend bumpmap          
		map textures/reflex/special/launchpad01_treads_local
            scroll    time * -1.4, time* 0.0
	}
      {
            blend add
            map textures/reflex/special/launchpad01_add
color       0.94 * (jitter5 [time * 2]), 0.57 * (jitter5 [time * 2]), 0.13 * (jitter5 [time * 2]), 1.0
      }
      {
            blend diffusemap
            map textures/reflex/special/launchpad01
            alphaTest .5
      }
	{
		blend specularMap      
		map textures/reflex/special/launchpad01_spec
	}
	{
		blend bumpmap          
		map textures/reflex/special/launchpad01_local
	}
}

